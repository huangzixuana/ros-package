<?xml version="1.0" encoding="UTF-8"?>

<behavior name="BracketLinePlace">

	<executable package_path="comm_behaviors.bracketlineplace_sm" class="BracketLinePlaceSM" />
	<tagstring>test</tagstring>
	<author>zcx</author>
	<date>Wed May 15 2024</date>
	<description>
		bkt line place
	</description>


	<!-- Contained Behaviors -->
	<contains name="StopPVMDetect" />

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="pvm_width" default="1134" label="pvm_width" hint="Sets the pvm_width">
			<min value="0" />
			<max value="9999" />
		</param>

		<param type="numeric" name="install_gap" default="10" label="install_gap" hint="Sets the install_gap">
			<min value="-9999" />
			<max value="9999" />
		</param>

		<param type="numeric" name="place_solar_z" default="0.05" label="place_solar_z" hint="Sets the place_solar_z">
			<min value="-10.0" />
			<max value="10.0" />
		</param>

	</params>


</behavior>